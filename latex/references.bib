@article{ WOS:000432504700016,
Author = {Buyukozkan, Gulcin and Gocer, Fethullah},
Title = {Digital Supply Chain: Literature review and a proposed framework for
   future research},
Journal = {COMPUTERS IN INDUSTRY},
Year = {2018},
Volume = {97},
Pages = {157-177},
Month = {MAY},
Abstract = {Suppliers, partners, companies and dealers in supply chains do use,
   generate and share information with others. These associations lead to a
   multitude of challenges and opportunities within the supply chains. A
   Digital Supply Chain (DSC) is a smart, value-driven, efficient process
   to generate new forms of revenue and business value for organizations
   and to leverage new approaches with novel technological and analytical
   methods DSC is not about whether goods and services are digital or
   physical, it is about the way how supply chain processes are managed
   with a wide variety of innovative technologies, e.g. unmanned aerial
